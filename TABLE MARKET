CREATE TABLE MARKET (
  MARKET_CODE varchar(45) NOT NULL,
  MARKET_NAME varchar(45) DEFAULT NULL,
  ZONE varchar(45) DEFAULT NULL,
  PRIMARY KEY (MARKET_CODE)
)

